{
    "version":2,
    "builds":[
        {
            "src":"./index.js",
            "use":"@vercel/node"
        }
    ],
    "routes": [
      { 
          "src": "/(.*)",
          "dest": "/" 
      }
    ],
    "functions": {
    "/": {
      "maxDuration": 30
    },
    "/login": {
      "maxDuration": 30
    },
    "/signup": {
      "maxDuration": 30
    }
  }
  }
